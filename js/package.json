{
  "name": "weave",
  "version": "0.6.8",
  "description": "AI development toolkit",
  "types": "dist/src/index.d.ts",
  "main": "dist/src/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "watch": "nodemon --watch 'src/**/*.ts' --watch 'testing/**/*.ts' --watch 'testing/**/*.mjs' --exec 'ts-node' testing/predict.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wandb/weave/js"
  },
  "author": "",
  "license": "Apache-2.0",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "cli-progress": "^3.12.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.57.0",
    "uuidv7": "^1.0.1",
    "weave": "^0.6.8"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "ts-jest": "^29.2.5"
  }
}
